<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <title>Credential Transparency Initiative | @ViewBag.Title</title>
	<link rel="icon" type="image/png" href="@Url.Content( "~/images/common/favicon.png" )" />
    <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.11.1.js"></script>
    <script src="~/Scripts/jquery.timepicker.js"></script>
	<script type="text/javascript" src="@Url.Content( "~/Scripts/common.js" )?ver=2"></script>
    <link href="@Url.Content( "~/Styles/common.css" )?ver=3" rel="stylesheet" type="text/css" />
    <script type="text/javascript">
    	if (typeof (console) == "undefined") {
    		var console = { log: function () { return; } };
    	}
    </script>
      @RenderSection( "css", false )
  </head>
  <body>

    <div id="header">
      @Html.Partial( "~/Views/Shared/Header.cshtml" )
    </div>
    <div id="content" @Html.Raw( ViewBag.Theme != null && ViewBag.Theme.ToLower() == "light" ? "class=\"light\"" : "" )>
      @RenderBody()
    </div>
    <div id="footer">
      @Html.Partial( "~/Views/Shared/Footer.cshtml" )
    </div>

	@Html.Partial( "~/Views/Shared/_ConsoleMessage.cshtml" )
    @RenderSection( "scripts", required: false )
    @Html.Partial( "~/Views/Shared/_ModalWindows.cshtml" )
    @Html.Partial( "~/Views/Shared/_ToolTips.cshtml" )

    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
      ga('create', 'UA-77712853-2', 'auto');
      ga('send', 'pageview');
    </script>

  </body>
</html>